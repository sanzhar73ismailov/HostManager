<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Action</servlet-name>
        <servlet-class>kz.biostat.lishostmanager.web.lab.servlet.Action</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Action</servlet-name>
        <url-pattern>/listInstruments</url-pattern>
        <url-pattern>/edit</url-pattern>
        <url-pattern>/Action</url-pattern>
        <url-pattern>/index</url-pattern>
        <url-pattern>/list</url-pattern>
        <url-pattern>/apparatus</url-pattern>
        <url-pattern>/changeStatus</url-pattern>
        <url-pattern>/listLogs</url-pattern>
        <url-pattern>/save</url-pattern>
        <url-pattern>/remove</url-pattern>
        <url-pattern>/listOrders</url-pattern>
        <url-pattern>/addOrder</url-pattern>
        <url-pattern>/deleteAll</url-pattern>
        <url-pattern>/sql</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <jsp-config>
        <jsp-property-group>
            <display-name>view</display-name>
            <url-pattern>/WEB-INF/view/*</url-pattern>
            <page-encoding>UTF-8</page-encoding>
            <include-prelude>/WEB-INF/jspf/header.jspf</include-prelude>
            <include-coda>/WEB-INF/jspf/bottom.jspf</include-coda>
        </jsp-property-group>
    </jsp-config>
</web-app>
